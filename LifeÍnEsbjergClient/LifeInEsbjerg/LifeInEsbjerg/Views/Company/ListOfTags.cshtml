@model IEnumerable<DtoModel.Tag>

<div class="row " style="margin:10px 0 10px 0;">

    @foreach (var item in Model)
            {
        <div style="text-decoration:none; float:left; margin-right:10px;">
            <button type="button" onclick="location.href='@Url.Action("Index", "Company", new { searchString = item.Name })'" class="btn btn-primary btn-xs" >@item.Name</button>
        </div>
    }

</div>